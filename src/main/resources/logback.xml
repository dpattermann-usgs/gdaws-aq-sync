<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss} gdaws-aq-sync %p: %m \(%c{1}\)%n</pattern>
		</encoder>
	</appender>
	
	<root level="ERROR">
		<appender-ref ref="stdout" />
	</root>
</configuration>